{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "esnext",
    "target": "es5",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "assets": [
        "assets"
      ],
      "src": [
        "src"
      ],
    }
  },
  "include": [
    "src/**/*"
  ]
}
