{
  "files.exclude": {
    "**/*.scss.d.ts": true,
  },
  "eslint.enable": true,
  "eslint.run": "onType",
  "eslint.packageManager": "yarn",
  "eslint.alwaysShowStatus": true,
  "typescript.format.insertSpaceAfterKeywordsInControlFlowStatements": false,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  }
}
